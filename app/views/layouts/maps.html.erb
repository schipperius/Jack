<!DOCTYPE html>

<html>

<head>
  <%= render 'layouts/head' %>
</head>

<body id="container" class="container-fluid" onload="initialize()">

  <nav role="navigation" class="navbar navbar-fixed-top">
    <%= render 'layouts/nav' %>
  </nav>

  <header role="banner" id="header" class="row-fluid">
    <hgroup id="hgroup">
      <h1><span>The Book of Jack</span></h1>
      <h2><span>An entertaining exploration of all things Jack</span></h2>
    </hgroup>
  </header>

  <section role="main" class="container-fluid">
    <div class="row-fluid">
      <section class="span12 columns">
        <%= yield %>
      </section>
    </div>
  </section>

  <footer role="contentinfo" class="navbar navbar-fixed-bottom">
    <%= render 'layouts/footer' %>
  </footer>

  <%= javascript_include_tag "application" %>

  <script type="text/javascript"  src="http://maps.googleapis.com/maps/api/js?key=AIzaSyAUtjRWrLuGoE4rD4RROzPLdzTg_vgb8VA&sensor=false">
  </script>

</body>

</html>
