<!DOCTYPE html>

<html>

<head>
  <%= render 'layouts/head' %>
</head>

<body id="container" class="container-fluid" onload="initialize()">

  <nav role="navigation" class="navbar navbar-fixed-top">
    <%= render 'layouts/nav' %>
  </nav>

  <header role="banner" id="header" class="row-fluid">
    <%= render 'layouts/header' %>
  </header>

  <section role="main" class="container-fluid">
    <div class="row-fluid">

      <section class="span9 columns">
        <%= yield %>
      </section>

      <aside role= "complementary" class="span3 columns">
        <%= render 'layouts/aside' %>
      </aside>

    </div>
  </section>

  <footer role="contentinfo" class="footer navbar navbar-fixed-bottom">
    <%= render 'layouts/footer' %>
  </footer>

  <%= javascript_include_tag "application" %>

  <script type="text/javascript"  src="http://maps.googleapis.com/maps/api/js?key=AIzaSyAUtjRWrLuGoE4rD4RROzPLdzTg_vgb8VA&amp;sensor=false">
  </script>

</body>

</html>
